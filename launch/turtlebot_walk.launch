<launch>
	<arg name = "record" default = "false"/>
	<include file="$(find turtlebot_gazebo)/launch/turtlebot_world.launch">
	<arg name="world_file" value="$(find turtlebot_gazebo)/worlds/corridor.world"/>
	</include>
	<node name="walker1" pkg="turtlebot_walk" type="walk"/>
	<node pkg="rosbag" type="record" name="rosbag_record_all" args="record -a -o data.bag -x /camera/(.*)" if="$(arg record)" />

</launch>
